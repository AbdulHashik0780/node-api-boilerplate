{
  "name": "node-api-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for web APIs using Node",
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=. NODE_ENV=production node index.js",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon index.js",
    "test": "cross-env NODE_PATH=. NODE_ENV=test mocha",
    "coverage": "cross-env NODE_PATH=. NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha",
    "lint": "eslint {src,test,config}/**/*.js",
    "sequelize": "cross-env NODE_PATH=. sequelize",
    "console": "cross-env NODE_PATH=. node scripts/console.js",
    "heroku-postbuild": "NODE_ENV=production NODE_PATH=. sequelize db:migrate --url=$DATABASE_URL"
  },
  "repository": {},
  "author": "Talysson <talyssonoc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@risingstack/trace": "^3.5.1",
    "awilix": "^2.2.3",
    "awilix-express": "^0.9.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "cross-env": "^3.2.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-status-monitor": "^0.1.9",
    "hpp": "^0.2.1",
    "http-status": "^1.0.1",
    "log4js": "^1.1.1",
    "method-override": "^2.3.7",
    "morgan": "^1.8.1",
    "pg": "^6.1.3",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "structure": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-change": "^2.1.2",
    "chance": "^1.0.6",
    "eslint": "^3.17.1",
    "factory-girl": "^3.1.0",
    "factory-girl-sequelize": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  }
}
